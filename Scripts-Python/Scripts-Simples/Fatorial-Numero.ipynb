    "#Calculando Fatorial de um Número\n",
    "\n",
    "fatorial = 1\n",
    "num = int(input('Entre com um número: '))\n",
    "while num > 1:\n",
    "    fatorial *= num\n",
    "    num -= 1\n",
    "print(f'Fatorial: {fatorial}')"
   ]
